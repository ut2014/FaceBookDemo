<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.it5.facebookdemo.demo2.MainActivity">
    <Button
        android:text="facebook_button"
        android:id="@+id/facebook_button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>
    <Button
        android:text="tibbr_button"
        android:id="@+id/tibbr_button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>
    <Button
        android:text="tibbr_forget_button"
        android:id="@+id/tibbr_forget_button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>
    <Button
        android:text="tibbr_reg_button"
        android:id="@+id/tibbr_reg_button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>

    <Button
        android:layout_marginTop="10dp"
        android:text="getAuth_token"
        android:id="@+id/auth_token"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>


</LinearLayout>
